{
  "name": "@mcp-z/mcp-outlook",
  "version": "1.0.6",
  "description": "MCP server for Outlook integration with OAuth authentication, message search, and batch operations",
  "keywords": [
    "outlook",
    "email",
    "microsoft",
    "mcp",
    "ai",
    "mcp-server",
    "model context protocol",
    "oauth",
    "microsoft graph",
    "email client",
    "outlook api",
    "message search",
    "email automation",
    "ai agent",
    "claude",
    "llm",
    "batch operations",
    "calendar",
    "categories",
    "label management",
    "email management",
    "productivity",
    "automation",
    "typescript",
    "nodejs",
    "integration",
    "api client",
    "msal"
  ],
  "homepage": "https://github.com/mcp-z/mcp-outlook#readme",
  "bugs": {
    "url": "https://github.com/mcp-z/mcp-outlook/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-z/mcp-outlook.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff.info@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "bin": "bin/server.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc -p . --noEmit && tsds build",
    "format": "tsds format",
    "prepublish:check": "ncp",
    "prepublishOnly": "tsds validate",
    "test": "npm run test:unit && npm run test:integration",
    "test:engines": "nvu engines tsds test:node --no-timeouts 'test/**/*.test.ts'",
    "test:integration": "tsds test:node --no-timeouts 'test/integration/**/*.test.ts'",
    "test:setup": "node scripts/setup-test.ts",
    "test:unit": "tsds test:node --no-timeouts 'test/unit/**/*.test.ts'",
    "version": "tsds version"
  },
  "dependencies": {
    "@mcp-z/email": "^1.0.2",
    "@mcp-z/oauth": "^1.0.1",
    "@mcp-z/oauth-microsoft": "^1.0.4",
    "@mcp-z/server": "^1.0.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "cors": "^2.8.5",
    "csv-stringify": "^6.6.0",
    "express": "^5.2.1",
    "keyv-registry": "^0.4.0",
    "module-root-sync": "^2.0.2",
    "pino": "^10.1.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@mcp-z/client": "^1.0.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "get-port": "^7.1.0",
    "keyv": "^5.5.5",
    "node-version-use": "^2.4.7",
    "ts-dev-stack": "^1.22.1",
    "tsds-config": "^1.0.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "mcpName": "io.github.mcp-z/mcp-outlook",
  "tsds": {
    "source": "src/index.ts"
  }
}
